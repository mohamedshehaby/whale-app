include ../mixins/_primaryButton

div(class="w-full bg-white rounded-xl shadow-sm dark:bg-gray-800 p-2 md:p-4")
    div(class="flex flex-col gap-6 ")
        // Table header
        div(class="flex justify-between items-center")
            h2(class="text-lg font-medium text-shark") Current Trips
            div(class="flex gap-4 justify-between items-center")
                div(class="relative")
                    div(class="absolute inset-y-0 start-0 flex items-center ps-3 pointer-events-none")
                        <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M10.875 18.75C15.2242 18.75 18.75 15.2242 18.75 10.875C18.75 6.52576 15.2242 3 10.875 3C6.52576 3 3 6.52576 3 10.875C3 15.2242 6.52576 18.75 10.875 18.75Z" stroke="#1D2026" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M16.4431 16.4434L20.9994 20.9997" stroke="#1D2026" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        </svg>
                    input(
                        type="search"
                        id="search"
                        placeholder="Search"
                        required
                        class="block w-full p-3 ps-10 text-base text-gray-900 border border-athens-gray  rounded-xl bg-white focus:ring-blue-500 focus:border-blue-500 "
                    )
                div(class="cursor-pointer flex justify-between items-center border border-athens-gray bg-white h-12 w-12 rounded-full")
                    <svg class="mx-auto" width="20" height="18" viewBox="0 0 20 18" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M8.1001 11.0166L7.98779 10.8789L1.12451 2.49316C0.911927 2.23213 0.846992 1.88195 0.944824 1.56836L0.996582 1.43652L0.998535 1.43262C1.16367 1.08232 1.51248 0.860352 1.8999 0.860352H18.1021C18.441 0.860442 18.7504 1.03044 18.9331 1.30762L19.0034 1.43262C19.1482 1.73981 19.128 2.09398 18.9565 2.37891L18.8735 2.49609L12.0142 10.8789L11.9009 11.0166V15.9619C11.9009 16.1943 11.7871 16.4086 11.6001 16.5371L11.5151 16.5869C11.3031 16.6919 11.0614 16.6851 10.8638 16.5762L10.7817 16.5225H10.7808L8.38037 14.7217L8.37842 14.7207L8.31592 14.6689C8.17952 14.5401 8.10014 14.3576 8.1001 14.1621V11.0166Z" stroke="#1D6DB6"/>
                    </svg>

                +primaryButton('Add')




        // Table body
        div(class="relative overflow-x-auto")
            table(class="w-full text-sm text-left rtl:text-right text-gray-500 ")
                thead(class="text-sm text-storm-gray uppercase bg-selago ")
                    tr
                        th(scope="col" class="px-6 py-3") image
                        th(scope="col" class="px-6 py-3") ID Trip
                        th(scope="col" class="px-6 py-3") Permit
                        th(scope="col" class="px-6 py-3") Captain
                        th(scope="col" class="px-6 py-3") Duration
                        th(scope="col" class="px-6 py-3") Escorts
                        th(scope="col" class="px-6 py-3") Status
                        th(scope="col" class="px-6 py-3") procedures


                tbody
                    tr(class="bg-white border-b  border-gray-200")
                        td(class="px-6 py-4")
                            img(class="w-12 h-8 rounded-sm" src="../assets/images/user-image.png" alt="Default avatar")
                        td(class="px-6 py-4")
                            | #12334
                        td(class="px-6 py-4")
                            span(class="text-forest-green font-medium") active
                        td(class="px-6 py-4") Ahmed Ali
                        td(class="px-6 py-4") 5 days
                        td(class="px-6 py-4") 5 fishermen
                        td(class="px-6 py-4")
                            span(class="bg-catskill-white text-shark text-xs font-medium px-4 py-1 rounded-lg") Back
                        td(class="px-6 py-4")
                            div(class="flex gap-2")
                                div(class="cursor-pointer flex w-8 h-8 rounded-full bg-selago justify-center items-center")
                                    <svg width="12" height="9" viewBox="0 0 12 9" fill="none" xmlns="http://www.w3.org/2000/svg">
